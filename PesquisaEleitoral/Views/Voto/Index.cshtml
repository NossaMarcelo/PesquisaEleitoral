@model IEnumerable<PesquisaEleitoral.Models.Candidato>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Votar", "voto")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CandidatoNome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CandidatoNumero)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CandidatoNome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CandidatoNumero)
            </td>
        </tr>
    }

</table>

